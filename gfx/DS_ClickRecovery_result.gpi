   #!/usr/bin/env gnuplot -persist
# DS Click Recovery results
#
# gnuplot is available from http://www.gnuplot.info
## FONTSIZE 16
set terminal postscript eps enhanced defaultplex leveldefault color solid dashlength 1.0 linewidth 2.0 butt noclip palfuncparam 2000,0.003  "Helvetica" 18
set output "gfx/DS_ClickRecovery_result.eps"
currpath = "`pwd| sed 's/gfx//'`"
load currpath."/gfx/default.gpi"
#set line style 1 lc rgb '#0060ad' lt 1 lw 2 pt 7 ps 1.5 
#set line style 2 lc rgb '#dd181f' lt 1 lw 2 pt 5 ps 1.5 
#set line style 3 lc rgb '#00ad06' lt 1 lw 2 pt 9 ps 1.5 
set border 3 linewidth 1.5
set ytics nomirror
set xtics nomirror
unset y2tics
unset x2tics
#set tmargin 0
#set rmargin 0
#set size noratio
set xlabel "Probe Delay (ms)" font "Helvetica,18"
set ylabel "Normalised Rate (spikes/s)" font "Helvetica,18"
set xrange [0:18]
set yrange [0:1.2]
unset key
set style line 1 lt 1 lw 1 pt 0 pi -1 ps 2  lc rgb '#BBBBBB'
set style line 2 lt 1 lw 3 pt 2 ps 4 lc rgb '#0060ad'
set style line 3 lt 1 lw 3 pt 3 pi 1 ps 4
set style line 4 lt 1 lw 3 pt 7 ps 4
#
plot "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit0.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit1.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit2.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit3.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit4.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit5.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit6.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit7.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit8.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit9.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit10.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit11.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit12.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit13.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit14.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit15.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit16.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit17.dat" using 1:5 with l notitle ls 1, \
 "../../cnstellate/DS_ClickRecovery/DS_ClickRecovery.Fit18.dat" using 1:5 with l notitle ls 1, \
